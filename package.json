{
  "name": "react-cv",
  "version": "1.2.1",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/crisboarna/react-cv.git"
  },
  "author": "Cristian Boarna <contact@crisboarna.com>",
  "bugs": {
    "url": "https://github.com/crisboarna/react-cv/issues"
  },
  "homepage": "https://cv.crisboarna.com",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "nx run-many --all --target=lint",
    "build": "nx run-many --all --target=build && nx build-exporter-layer infra",
    "test": "nx run-many --all --target=test",
    "release": "semantic-release"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "chrome-aws-lambda": "^10.1.0",
    "core-js": "^3.6.5",
    "express": "4.17.3",
    "puppeteer-core": "^13.5.2",
    "query-string": "^7.1.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-github-btn": "^1.2.2",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@mhlabs/cfn-diagram": "^1.1.33",
    "@nrwl/cli": "13.10.2",
    "@nrwl/cypress": "14.7.16",
    "@nrwl/eslint-plugin-nx": "13.10.2",
    "@nrwl/express": "13.10.2",
    "@nrwl/jest": "13.10.2",
    "@nrwl/linter": "13.10.2",
    "@nrwl/node": "13.10.2",
    "@nrwl/react": "13.10.2",
    "@nrwl/web": "13.10.2",
    "@nrwl/workspace": "13.10.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "7.0.2",
    "@types/aws-lambda": "^8.10.93",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "aws-cdk": "^2.43.1",
    "aws-cdk-lib": "2.43.1",
    "aws-cdk-lib-util": "^1.1.2",
    "aws-cdk-local": "^2.15.0",
    "aws-sdk": "^2.1118.0",
    "babel-jest": "27.2.3",
    "cdk-nag": "^2.12.32",
    "constructs": "^10.0.0",
    "cypress": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "nx": "15.6.2",
    "prettier": "^2.5.1",
    "puppeteer": "^13.5.2",
    "react-test-renderer": "18.0.0",
    "semantic-release": "^19.0.5",
    "source-map-support": "^0.5.16",
    "ts-jest": "27.0.5",
    "tsconfig-paths": "^3.14.1",
    "typescript": "~4.6.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ],
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
